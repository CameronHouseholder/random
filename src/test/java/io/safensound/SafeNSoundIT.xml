<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="SafeNSound" verbose="1">
    <test name="SafeNSound Google Search - Chrome">
        <parameter name="browser" value="chrome" />
        <classes>
            <class name="io.safensound.SafeNSoundGoogleSearchIT" />
        </classes>
    </test>
    <test name="SafeNSound Google Search - Firefox">
        <parameter name="browser" value="firefox" />
        <classes>
            <class name="io.safensound.SafeNSoundGoogleSearchIT" />
        </classes>
    </test>
</suite>